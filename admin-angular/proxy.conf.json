{
  "/api/*": {
    "target": "http://127.0.0.1:8000",
    "secure": false,
    "changeOrigin": false
  },
  "/auth/login": {
    "target": "http://127.0.0.1:8000",
    "secure": false,
    "changeOrigin": false,
    "pathRewrite": {
      "^/auth": ""
    }
  },
  "/auth/logout": {
    "target": "http://127.0.0.1:8000",
    "secure": false,
    "changeOrigin": false,
    "pathRewrite": {
      "^/auth": ""
    }
  },
  "/sanctum/csrf-cookie": {
    "target": "http://127.0.0.1:8000",
    "secure": false,
    "changeOrigin": false
  }
}
